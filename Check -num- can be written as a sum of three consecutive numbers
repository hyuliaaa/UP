#include <iostream>
using namespace std;

///function to check if a number can be
/// written as sum of three consecutive
/// integers.

int main()
{

    int n;
    cin>>n;

    if (n%3==0)
    {
        cout<<n/3-1<<" "<<n/3 <<" "<< n/3+1;
    }
    else
    {
        cout<<"-1";
    }



    return 0;

}
