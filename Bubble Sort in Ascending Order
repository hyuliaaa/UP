#include <iostream>

using namespace std;


int main()
{
    int arr[]={4,2,8,7,9};
    int len;
    len=sizeof(arr)/sizeof(arr[0]);

    for (int i=0; i<len; i++)
    {
        for (int j=0; j<len-1; j++)
        {
            if (arr[j]>arr[i+1])
            {
                int temp=arr[j];
                arr[j]=arr[i+1];
                arr[i+1]=temp;
            }
        }
    }
    for (int i=0; i<len; i++)
    {
        cout<<arr[i]<<" ";
    }

    return 0;
}
