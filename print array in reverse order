#include<iostream>
#include <cmath>

using namespace std;

const int SZ=100;

void inputArray(int arr[], int n)
{
    for(int i=0; i<n; i++)
    {
        cin>>arr[i];
    }
}



void printArray(int arr[], int n)
{
    for(int i=0; i<n; i++)
    {
        cout<<arr[i]<<" ";
    }
}

void printReverseOrder(int arr[], int n)
{
    for(int i=n-1; i>=0; i--)
    {
        cout<<arr[i]<<" ";
    }
}




int main()
{

    int arr[SZ];
    int n;
    cin>>n;
    inputArray(arr,n);

    printArray(arr,n);
    cout<<endl;
    printReverseOrder(arr,n);









    return 0;
}




