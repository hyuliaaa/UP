#include <iostream>
using namespace std;

/// check if a number can be
/// written as sum of three consecutive
/// integers.

int main()
{

    int n;
    cin>>n;

    if (n%3==0)
    {
        cout<<n/3-1<<" "<<n/3 <<" "<< n/3+1;
    }
    else
    {
        cout<<"-1";
    }



    return 0;

}
///check if a number can be written as a sum of five consecutive numbers
#include <iostream>
using namespace std;



int main()
{

    int n;
    cin>>n;

    if (n%5==0)
    {
        cout<<n/5-2<<" "<<n/5-1<<" "<<n/5 <<" "<< n/5+1<<" "<<n/5+2;
    }
    else
    {
        cout<<"-1";
    }



    return 0;

}
