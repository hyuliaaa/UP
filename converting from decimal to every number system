#include<iostream>
#include <cmath>

using namespace std;

int main()

{

    int num;
    cin>>num;
    int i=1;
    int everySystem=0;
    int system;
    cin>>system;


    while(num!=0)
    {
        int digit=num%system;
        everySystem+=digit*i;
        i*=10;
        num/=system;

    }
    cout<<everySystem;



    return 0;
}




