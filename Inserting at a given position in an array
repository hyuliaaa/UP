#include <iostream>
const int SZ=100;

using namespace std;

void inputArray(int arr[], int len)
{
    for(int i=0; i<len; i++)
    {
        cin>>arr[i];
    }
}

void insertingElement(int arr[], int &len, int index, int x)
{


    for(int i=len; i>index; i--)
    {
        arr[i]=arr[i-1];
    }

    arr[index]=x;
    len++;




}

void printArray(int arr[], int n)
{
    for(int i=0; i<n; i++)
    {
        cout<<arr[i]<<" ";
    }
}
int main()
{
    int arr[SZ];
    int len;
    cin>>len;
    inputArray(arr,len);
    insertingElement(arr,len, 4, 15);
    printArray(arr,len);







    return 0;
}
