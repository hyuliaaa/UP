#include <iostream>
#include <cstring>
#include <cstring>
using namespace std;
const int SZ=100;




int main()
{

    int n;
    cin>>n;
    int arr[SZ];
    for(int i=0; i<n; i++)
    {
        cin>>arr[i];
    }

    int x;
    cin>>x;

    int numbers=0;

    for(int i=0; i<n; i++)
    {
        for(int j=i; j<n; j++)
        {
            if(x==arr[i]+arr[j])
            {
                cout<<"arr["<<i<<"] "<<arr[i];
                cout<<" arr["<<j<<"] "<<arr[j]<<endl;
                cout<<arr[i]+arr[j]<<endl;
                numbers++;
            }
        }
    }

    cout<<numbers;











    return 0;
}
