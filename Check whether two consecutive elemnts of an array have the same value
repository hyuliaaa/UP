#include <iostream>

using namespace std;
const int SZ=100;
int main()
{

    int n;
    cin>>n;
    int arr[SZ];
    bool twoConsecutive=false;

    for (int i=0; i<n ; i++)
    {
        cin>>arr[i];
    }

    for(int i=0; i<n ; i++)
    {
        if(arr[i]==arr[i+1])
        {
            twoConsecutive=true;
            break;
        }
    }
    if(twoConsecutive)
    {
        cout<<"YES";
    }
    else
    {
        cout<<"no";
    }




    return 0;
}
