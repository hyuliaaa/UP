#include<iostream>
#include <cmath>

using namespace std;

const int SZ=100;

void inputArray(int arr[], int n)
{
    for(int i=0; i<n; i++)
    {
        cin>>arr[i];
    }
}



void printArray(int arr[], int n)
{
    for(int i=0; i<n; i++)
    {
        cout<<arr[i]<<" ";
    }
}

void reverseArray(int arr[], int n)
{
    for(int i=0; i<n/2; i++)
    {
        int temp=arr[i];
        arr[i]=arr[n-1-i];
        arr[n-1-i]=temp;
    }
}




int main()
{

    int arr[SZ];
    int n;
    cin>>n;
    inputArray(arr,n);

    printArray(arr,n);
    cout<<endl;
    reverseArray(arr,n);
    printArray(arr,n);









    return 0;
}




